package assignment;

import java.util.Scanner;

public class FibonacciSeries {
	public static void main(String[] args){
		int i,n,n1=0,n2=1,n3;
		System.out.println("enter the limit");
		Scanner in=new Scanner(System.in);
		n=in.nextInt();
		System.out.print(n2+" ");
		for(i=2;i<=n;i++)
		{
			n3=n1+n2;
			System.out.print(n3+" ");
			n1=n2;
			n2=n3;
		}
}
}
